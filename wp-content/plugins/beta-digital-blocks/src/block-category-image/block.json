{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/category-image",
	"version": "0.1.0",
	"title": "Imagem destacada - categoria",
	"category": "beta-digital-category",
	"icon": "smiley",
	"description": "Insira no carrossel de categoria de produtos",
	"supports": {
		"html": false
	},
	"textdomain": "category-image",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"categoryLink": {
			"type": "string"
		},
		"categoryName": {
			"type": "string",
			"source": "text",
			"selector": "p"
		},
		"imageUrl": {
			"type": "string"
		},
		"imageWidth": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "width"
		},
		"imageHeight": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "height"
		},
		"imageId": {
			"type": "number"
		},
		"imageAlt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		}
	}
}
